{
  "name": "mind-git",
  "version": "1.1.0",
  "description": "Mathematical foundation for self-evolving computational systems with CanvasL visual programming",
  "main": "logos-system/dist/index.js",
  "types": "logos-system/dist/index.d.ts",
  "bin": {
    "mind-git": "./bin/mind-git.js"
  },
  "scripts": {
    "build": "cd logos-system && npm run build",
    "test": "cd logos-system && npm test",
    "test:unit": "cd logos-system && npm run test:unit",
    "test:formal": "cd logos-system/formal && make verify",
    "dev": "cd logos-system && npm run dev",
    "lint": "cd logos-system && npm run lint",
    "clean": "cd logos-system && npm run clean && cd formal && make clean"
  },
  "keywords": [
    "canvasl",
    "polynomial",
    "mathematics",
    "visual-programming",
    "compiler",
    "ast",
    "formal-verification",
    "spatial-computing",
    "typescript",
    "javascript"
  ],
  "author": "Brian Thorne <bthornemail@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bthornemail/mind-git.git"
  },
  "type": "commonjs",
  "dependencies": {
    "@trbn/jsoncanvas": "^1.0.6",
    "commander": "^11.0.0"
  }
}
