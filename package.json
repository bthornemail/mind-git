{
  "name": "mind-git",
  "version": "1.1.0",
  "description": "Mathematical foundation for self-evolving computational systems with CanvasL visual programming",
  "main": "logos-system/dist/index.js",
  "types": "logos-system/dist/index.d.ts",
  "bin": {
    "mind-git": "./bin/mind-git-simple.cjs",
    "mind-git-metadata": "./bin/mind-git-metadata.cjs"
  },
  "type": "module",
  "scripts": {
    "build": "cd logos-system && npm run build",
    "test": "cd logos-system && npm test",
    "start": "node logos-system/dist/index.js",
    "metadata:update": "cd metadata && node scripts/integrate-metadata-simple.js",
    "metadata:export": "cd metadata && node scripts/export-system-simple.js export",
    "metadata:status": "cd metadata && node scripts/cli.js status"
  },
  "keywords": [
    "canvasl",
    "polynomial",
    "mathematics",
    "visual-programming",
    "compiler",
    "ast",
    "formal-verification",
    "spatial-computing",
    "typescript",
    "javascript"
  ],
  "author": "Brian Thorne <bthornemail@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bthornemail/mind-git.git"
  },
  "files": [
    "bin/",
    "examples/",
    "logos-system/dist/",
    "logos-system/package.json",
    "README.md",
    "LICENSE",
    "json.canvas.d.ts",
    "json.canvas.js"
  ],
  "dependencies": {
    "@trbn/jsoncanvas": "^1.0.6",
    "commander": "^11.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}