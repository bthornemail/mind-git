{
  "name": "mind-git",
  "version": "2.0.5",
  "description": "World's most advanced VR spatial programming ecosystem with AI intelligence, E8 lattice routing, and immersive WebXR interfaces",
  "main": "logos-system/dist/index.js",
  "types": "logos-system/dist/index.d.ts",
  "bin": {
    "mind-git": "bin/mind-git-simple.cjs",
    "mind-git-metadata": "bin/mind-git-metadata.cjs",
    "universal-kernel": "universal-metadata-kernel.js",
    "universal-exporter": "universal-exporter.js",
    "pfister-inference": "pfister-inference-engine.js",
    "mind-git-opencode": ".opencode/client.js"
  },
  "type": "module",
  "scripts": {
    "build": "cd logos-system && npm run build",
    "test": "cd logos-system && npm test",
    "start": "node logos-system/dist/index.js",
    "metadata:update": "cd metadata && node scripts/integrate-metadata-simple.js",
    "metadata:export": "cd metadata && node scripts/export-system-simple.js export",
    "metadata:status": "cd metadata && node scripts/cli.js status",
    "deploy:docker": "docker-compose -f docker-compose.vr-ecosystem.yml up -d",
    "deploy:k8s": "kubectl apply -f k8s/vr-ecosystem.yml",
    "deploy:monitoring": "kubectl apply -f k8s/monitoring.yml",
    "docs:generate": "npm run docs:build",
    "docs:serve": "npm run docs:serve",
    "health:all": "npm run health:all",
    "test:ecosystem": "npm run test:ecosystem",
    "benchmark": "npm run test:performance",
    "opencode:status": "node .opencode/client.js status",
    "opencode:install": "cd .opencode && npm install",
    "opencode:test": "node .opencode/client.js tools && node .opencode/client.js agents"
  },
  "keywords": [
    "canvasl",
    "polynomial",
    "mathematics",
    "visual-programming",
    "compiler",
    "ast",
    "formal-verification",
    "spatial-computing",
    "typescript",
    "javascript",
    "vr-spatial-programming",
    "ai-intelligence",
    "h2gnn",
    "hyperbolic-geometry",
    "e8-lattice",
    "webxr",
    "immersive-vr",
    "meta-log-orchestrator",
    "knowledge-graph",
    "natural-language",
    "kubernetes",
    "docker-ecosystem",
    "production-ready",
    "opencode",
    "ai-integration"
  ],
  "author": {
    "name": "Brian Thorne",
    "email": "bthornemail@gmail.com",
    "url": "https://github.com/bthornemail"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bthornemail/mind-git.git"
  },
  "files": [
    "bin/",
    "examples/",
    "logos-system/dist/",
    "logos-system/package.json",
    ".opencode/",
    "README.md",
    "LICENSE",
    "json.canvas.d.ts",
    "json.canvas.js",
    "universal-metadata-kernel.js",
    "universal-exporter.js",
    "pfister-inference-engine.js",
    "UNIVERSAL-KERNEL-README.md",
    "PFISTER-32-README.md"
  ],
  "dependencies": {
    "@trbn/jsoncanvas": "^1.0.6",
    "commander": "^11.0.0",
    "kubernetes": "^1.28.0",
    "@kubernetes/client": "^1.28.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "@opencode-ai/sdk": "^1.0.0"
  }
}
