{
  "name": "meta-log",
  "version": "1.0.0",
  "description": "Central nervous system for VR spatial programming ecosystem",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --project tsconfig.json --watch",
    "start": "node dist/index.js",
    "test": "jest src/",
    "test:integration": "jest tests/integration/",
    "test:performance": "node --expose-gc tests/performance/benchmark.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docs": "typedoc src --out docs/api",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "orchestration",
    "e8-lattice",
    "knowledge-graph",
    "natural-language",
    "spatial-programming",
    "vr-ecosystem",
    "ai-coordination",
    "workflow-automation",
    "geometric-routing"
  ],
  "author": "Brian Thorne <bthornemail@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bthornemail/meta-log.git"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "docs/"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/express": "^4.17.20",
    "@types/ws": "^8.5.8",
    "@types/uuid": "^9.0.6",
    "@types/lodash": "^4.14.200",
    "typescript": "^5.2.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.6",
    "ts-jest": "^29.1.1",
    "eslint": "^8.52.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}