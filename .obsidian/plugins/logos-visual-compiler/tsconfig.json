{
  "compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "ES2017",
		"allowJs": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": false,
		"lib": [
			"DOM",
			"ES2017"
		],
		"esModuleInterop": true,
		"skipLibCheck": true
	}
}
